{
  "name": "jvcalderon/TokenService",
  "type": "project",
  "description": "A dockerized microservice who provides a REST interface with token's common functionality",
  "keywords": ["Token", "Microservice", "DDD", "Hexagonal architecture"],
  "license": "MIT",
  "authors": [
    {
      "name": "José Vte. Calderón",
      "email": "jvcalcab@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "silex/silex": "~2.0",
    "carlosbuenosvinos/ddd": "^1.6",
    "doctrine/orm": "^2.5",
    "symfony/yaml": "^3.2",
    "willdurand/hateoas": "^2.10",
    "jms/serializer": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*"
  },
  "autoload": {
    "psr-0": {
      "Token": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Token": "tests/Token/"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
